---
import mtnMudIcon from '~/assets/images/Logos/mtn-mud-icon.png';
---

<!-- ICO favicon for Google and legacy browsers (must come first) -->
<link rel="icon" href="/favicon.ico" sizes="32x32" />
<link rel="icon" href="/favicon.ico" sizes="16x16" />
<!-- PNG favicon for modern browsers -->
<link rel="icon" type="image/png" href={mtnMudIcon.src} />
<link rel="mask-icon" href={mtnMudIcon.src} color="#DD5E26" />
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
<link rel="manifest" href="/site.webmanifest" />
